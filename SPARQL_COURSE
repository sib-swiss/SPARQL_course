PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
select * where {
    ?s skos:prefLabel ?o .
} limit 100

PREFIX up: <http://purl.uniprot.org/core/>
select * where {
    ?s up:fullName ?o .
} limit 100

PREFIX up: <http://purl.uniprot.org/core/>
select * where {
    ?s up:catalyzedReaction ?o .
} limit 100

PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX up: <http://purl.uniprot.org/core/>
select * where {
    ?CatalyticActivity rdf:type up:Catalytic_Activity ;
                       up:enzymeClass ?EC ;
    				   up:catalyzedReaction ?rhea
} limit 100


